 function evenOccurence (arr){
  var sorted = arr.sort();
  console.log(sorted);
  
  var possible = [];
  for (var i= 0: <sorted.length; i++) {
if (sorted[i]=== sorted[i+1]) {
possible.push(sorted)[i]);	
}
  };


 };
 evenOccurence([1,6,2,4,5,6,8,9,6]);